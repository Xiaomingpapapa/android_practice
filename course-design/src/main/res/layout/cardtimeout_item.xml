<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/carduser_item_row"
		android:background="@color/listbg"
		android:layout_width="fill_parent"
		android:layout_height="60.0dip"
		android:gravity="center_vertical">
		<TextView 
			android:id="@+id/cardtimeout_item_cardsn"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="5.0dip"
			android:textColor="#ff000000"
			android:ellipsize="end" android:singleLine="true"
			android:text="admin"/>
		<TextView
			android:id="@+id/cardtimeout_item_userid"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:ellipsize="end" android:singleLine="true"
			android:paddingRight="5.0dip"
			android:textColor="#ff000000"
			android:text="admin"/>
		<TextView 
			android:id="@+id/cardtimeout_item_position"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="5.0dip"
			android:layout_marginTop="5.0dip"
			android:textColor="#ff000000"
			android:ellipsize="end" android:singleLine="true"
			android:layout_below="@id/cardtimeout_item_userid"
			android:text="admin"/>
		<TextView
			android:id="@+id/cardtimeout_item_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="5.0dip"
			android:layout_alignParentRight="true"
			android:ellipsize="end" android:singleLine="true"
			android:paddingRight="5.0dip"
			android:textColor="#ff000000"
			android:layout_below="@id/cardtimeout_item_userid"
			android:text="admin"/>
	</RelativeLayout>