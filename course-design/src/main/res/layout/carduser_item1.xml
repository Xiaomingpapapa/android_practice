<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/carduser_item_row"
		android:background="@color/listbg"
		android:layout_width="fill_parent"
		android:layout_height="100.0dip"
		android:gravity="center_vertical">
		<TextView 
			android:id="@+id/carduser_item1_cardsn"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="10.0dip"
			android:textColor="#ff000000"
			android:ellipsize="end" android:singleLine="true"
			android:text="admin"/>
		<TextView
			android:id="@+id/carduser_item1_userid"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:ellipsize="end" android:singleLine="true"
			android:paddingRight="10.0dip"
			android:textColor="#ff000000"
			android:text="admin"/>
		<TextView 
			android:id="@+id/carduser_item1_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="10.0dip"
			android:textColor="#ff000000"
			android:ellipsize="end" android:singleLine="true"
			android:layout_below="@id/carduser_item1_cardsn"
			android:text="admin"/>
		<TextView
			android:id="@+id/carduser_item1_state"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:ellipsize="end" android:singleLine="true"
			android:paddingRight="10.0dip"
			android:textColor="#ff000000"
			android:layout_below="@id/carduser_item1_userid"
			android:text="admin"/>
		<TextView 
			android:id="@+id/carduser_item1_department"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="10.0dip"
			android:layout_marginBottom="5.0dip"
			android:textColor="#ff000000"
			android:ellipsize="end" android:singleLine="true"
			android:layout_below="@id/carduser_item1_name"
			android:text="admin"/>
		<TextView
			android:id="@+id/carduser_item1_position"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="5.0dip"
			android:layout_alignParentRight="true"
			android:ellipsize="end" android:singleLine="true"
			android:paddingRight="10.0dip"
			android:textColor="#ff000000"
			android:layout_below="@id/carduser_item1_state"
			android:text="admin"/>
	</RelativeLayout>